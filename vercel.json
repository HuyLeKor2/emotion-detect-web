{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "models/**",
            "use": "@vercel/static"
        },
        {
            "src": "icons/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/models/(.*)",
            "dest": "/models/$1"
        },
        {
            "src": "/icons/(.*)",
            "dest": "/icons/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "headers": [
        {
            "source": "/models/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}