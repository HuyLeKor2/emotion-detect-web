{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/models/:path*",
            "destination": "/public/models/:path*"
        },
        {
            "source": "/icons/:path*",
            "destination": "/public/icons/:path*"
        },
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "header",
                    "key": "accept",
                    "value": "text/html"
                }
            ],
            "destination": "/public/index.html"
        }
    ],
    "headers": [
        {
            "source": "/models/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}