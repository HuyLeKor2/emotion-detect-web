{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "models/**",
            "use": "@vercel/static"
        },
        {
            "src": "icons/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/models/:path*",
            "destination": "/models/:path*"
        },
        {
            "source": "/icons/:path*",
            "destination": "/icons/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/models/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}